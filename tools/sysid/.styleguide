cppHeaderFileInclude {
  \.h$
  \.hpp$
  \.inc$
  \.inl$
}

cppSrcFileInclude {
  \.cpp$
}

generatedFileExclude {
  src/main/native/resources/
  src/main/native/win/sysid.ico
  src/main/native/mac/sysid.icns
}

