  ^fmt/
  ^gtest/
  ^imgui
  ^implot\.h$
  ^portable-file-dialogs\.h$
  ^wpi/datalog/
  ^wpi/glass/
  ^wpi/ntcore/
  ^wpi/math/
  ^wpi/units/
  ^wpi/util/
  ^wpi/gui
}
